FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "system";
    local           "";

    class           dictionary;
    object          setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue Sal 0
    volScalarFieldValue Sed 0
);

regions
(
    boxToCell
    {
        box (0 0 -2) (1.8 0.405 2);

        fieldValues
        (
            volScalarFieldValue Sal 8 
        );
    }
);


